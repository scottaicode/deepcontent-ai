{
  "version": 2,
  "functions": {
    "src/app/api/scrape-website/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "src/app/api/document-analysis/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "src/app/api/youtube/transcript/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "src/app/api/youtube-direct/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/scrape-website",
      "dest": "/api/scrape-website",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/document-analysis",
      "dest": "/api/document-analysis",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/youtube/transcript",
      "dest": "/api/youtube/transcript",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/youtube-direct",
      "dest": "/api/youtube-direct",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm install && npx playwright install chromium --with-deps",
  "outputDirectory": ".next"
} 