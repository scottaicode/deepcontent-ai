{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production"
  },
  "github": {
    "silent": true
  },
  "rewrites": [
    { "source": "/create", "destination": "/create.html" },
    { "source": "/create/", "destination": "/create.html" }
  ],
  "routes": [
    { "src": "/create", "dest": "/create.html" },
    { "src": "/create/(.*)", "dest": "/create.html" }
  ]
}